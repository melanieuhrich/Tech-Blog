<div>
    <div>
        <h5>Log In</h5>
    </div>
    <div class="row">
        <form class="col s12 login-form">
            <div class="row">
                <div class="input-field col s12">
                    <input id="email" type="email" class="validate email-login">
                    <label for="email">Email</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <input id="password" type="password" class="validate password-login">
                    <label for="password">Password</label>
                </div>
            </div>
             <button type='submit' class="waves-effect waves-light btn-small">Log In</button>
        </form>
    </div>
</div>


<div>
    <div>
        <h5>Sign Up</h5>
    </div>
    <div class="row">
        <form class="col s12 signup-form">
            <div class="row">
                <div class="input-field col s12">
                    <input id="first_name" type="text" class="validate name-signup">
                    <label for="first_name">Name</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <input id="email" type="email" class="validate email-signup">
                    <label for="email">Email</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <input id="password" type="password" class="validate password-signup">
                    <label for="password">Password</label>
                </div>
            </div>
            <button type='submit' class="waves-effect waves-light btn-small">Sign Up</button>
        </form>
    </div>
</div>

<script src="./js/login.js"></script>